{"ast":null,"code":"import { reactive, ref, toRefs } from \"vue\";\nimport { mapState, mapGetters } from \"vuex\";\nexport default {\n  name: \"hero_showcase\",\n  components: {},\n  setup() {\n    const Heroes = [];\n    const data = reactive({\n      sortBy: \"A-Z\",\n      menuShow: false\n    });\n    return {\n      ...toRefs(data),\n      Heroes\n    };\n  },\n  //计算属性 类似于data概念\n  computed: {\n    //声明sorte仓库中的变量让其可以在组件中读取\n    ...mapState([\"heroes\"]),\n    ...mapState([\"regional\"]),\n    //声明gett方法\n    ...mapGetters([\"getDeepCloneHeroes\"])\n  },\n  //监听data中的数据变化\n  watch: {\n    sortBy(n, o) {\n      if (n === \"A-Z\") {\n        console.log(\"1\");\n        this.Heroes = this.Heroes.sort((a, b) => a.formerly - b.formerly);\n      } else if (n === \"最新\") {\n        console.log(\"最新\");\n        this.Heroes = this.Heroes.sort((a, b) => a.releaseDate - b.releaseDate);\n      } else if (n === \"地区\") {\n        console.log(\"地区\");\n        this.Heroes = this.Heroes.sort((a, b) => a.relevantRegion - b.relevantRegion);\n      }\n    }\n  },\n  //方法集合\n  methods: {\n    //下拉菜单\n    showMenu() {\n      if (!this.menuShow) {\n        this.$refs.sortmenu.style.transform = \"scaleY(1)\";\n      } else {\n        this.$refs.sortmenu.style.transform = \"scaleY(0)\";\n      }\n      this.menuShow = !this.menuShow;\n    }\n  },\n  //生命周期 - 创建完成（可以访问当前this实例）\n  created() {\n    this.Heroes = this.getDeepCloneHeroes;\n  },\n  //生命周期 - 挂载完成（可以访问DOM元素）\n  mounted() {},\n  beforeCreate() {},\n  //生命周期 - 创建之前\n  beforeMount() {},\n  //生命周期 - 挂载之前\n  beforeUpdate() {},\n  //生命周期 - 更新之前\n  updated() {},\n  //生命周期 - 更新之后\n  beforeDestroy() {},\n  //生命周期 - 销毁之前\n  destroyed() {},\n  //生命周期 - 销毁完成\n  activated() {} //如果页面有keep-alive缓存功能，这个函数会触发\n};","map":{"version":3,"names":["reactive","ref","toRefs","mapState","mapGetters","name","components","setup","Heroes","data","sortBy","menuShow","computed","watch","n","o","console","log","sort","a","b","formerly","releaseDate","relevantRegion","methods","showMenu","$refs","sortmenu","style","transform","created","getDeepCloneHeroes","mounted","beforeCreate","beforeMount","beforeUpdate","updated","beforeDestroy","destroyed","activated"],"sources":["D:\\MyCode\\Vuescode\\league_of_legends_universe\\src\\views\\hero_showcase.vue"],"sourcesContent":["<template>\r\n    <div id=\"bk\">\r\n    </div>\r\n    <div class=\"search_bar\">\r\n        <div class=\"search_input\">\r\n            <img src=\"@/assets/img/public_img/hero_crest_icon.png\">\r\n            <input\r\n                type=\"text\"\r\n                name=\"keyword\"\r\n                placeholder=\"找到一个英雄\"\r\n            >\r\n        </div>\r\n        <div\r\n            class=\"sort\"\r\n            @click=\"showMenu()\"\r\n        >\r\n            <img\r\n                src=\"../assets/img/public_img/下拉菜单.png\"\r\n                alt=\"\"\r\n            >\r\n            <p>筛选方式<span>：{{ sortBy }}</span></p>\r\n            <ul ref=\"sortmenu\">\r\n                <li @click=\"sortBy='A-Z'\">A-Z</li>\r\n                <li @click=\"sortBy='最新'\">最新</li>\r\n                <li @click=\"sortBy='地区'\">地区</li>\r\n            </ul>\r\n        </div>\r\n    </div>\r\n    <div id=\"box_head\">\r\n        <img src=\"@/assets/img/public_img/hero_crest_icon.png\">\r\n        <div>\r\n            <h1>英雄</h1>\r\n        </div>\r\n    </div>\r\n    <div id=\"hero_list\">\r\n        <router-link\r\n            v-for=\"(hero,index) in Heroes\"\r\n            :key=\"index\"\r\n            :to=\"`/HeroInfo/${hero.formerly}`\"\r\n        >\r\n            <div\r\n                class=\"hero_card\"\r\n                :style=\"{\r\n                  'background-image': 'url(' + require(`@/assets/img/hero_posters/${hero.posters}`) + ')',\r\n                  'background-position': `${hero.x*1.0/hero.width*100}% ${hero.y*1.0/hero.height*100}%`,\r\n                  'background-size': 'cover',\r\n                  }\"\r\n            >\r\n                <div>\r\n                    <div>\r\n                        <h2>{{ hero.translation }}</h2>\r\n                        <h1>{{ regional.find(item => item.formerly === hero.relevantRegion).translation }}</h1>\r\n                    </div>\r\n                    <div>\r\n                        <p>探索<span>⇢</span></p>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </router-link>\r\n    </div>\r\n\r\n</template>\r\n<script>\r\nimport { reactive, ref, toRefs } from \"vue\";\r\nimport { mapState, mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n    name: \"hero_showcase\",\r\n    components: {},\r\n    setup() {\r\n        const Heroes = [];\r\n        const data = reactive({\r\n            sortBy: \"A-Z\",\r\n            menuShow: false,\r\n        });\r\n        return {\r\n            ...toRefs(data),\r\n            Heroes,\r\n        };\r\n    },\r\n    //计算属性 类似于data概念\r\n    computed: {\r\n        //声明sorte仓库中的变量让其可以在组件中读取\r\n        ...mapState([\"heroes\"]),\r\n        ...mapState([\"regional\"]),\r\n        //声明gett方法\r\n        ...mapGetters([\"getDeepCloneHeroes\"]),\r\n    },\r\n    //监听data中的数据变化\r\n    watch: {\r\n        sortBy(n, o) {\r\n            if (n === \"A-Z\") {\r\n                console.log(\"1\");\r\n                this.Heroes = this.Heroes.sort(\r\n                    (a, b) => a.formerly - b.formerly\r\n                );\r\n            } else if (n === \"最新\") {\r\n              console.log(\"最新\");\r\n                this.Heroes = this.Heroes.sort(\r\n                    (a, b) => a.releaseDate - b.releaseDate\r\n                );\r\n            } else if (n === \"地区\") {\r\n              console.log(\"地区\");\r\n                this.Heroes = this.Heroes.sort(\r\n                    (a, b) => a.relevantRegion - b.relevantRegion\r\n                );\r\n            }\r\n        },\r\n    },\r\n    //方法集合\r\n    methods: {\r\n        //下拉菜单\r\n        showMenu() {\r\n            if (!this.menuShow) {\r\n                this.$refs.sortmenu.style.transform = \"scaleY(1)\";\r\n            } else {\r\n                this.$refs.sortmenu.style.transform = \"scaleY(0)\";\r\n            }\r\n            this.menuShow = !this.menuShow;\r\n        },\r\n    },\r\n    //生命周期 - 创建完成（可以访问当前this实例）\r\n    created() {\r\n        this.Heroes = this.getDeepCloneHeroes;\r\n    },\r\n    //生命周期 - 挂载完成（可以访问DOM元素）\r\n    mounted() {},\r\n    beforeCreate() {}, //生命周期 - 创建之前\r\n    beforeMount() {}, //生命周期 - 挂载之前\r\n    beforeUpdate() {}, //生命周期 - 更新之前\r\n    updated() {}, //生命周期 - 更新之后\r\n    beforeDestroy() {}, //生命周期 - 销毁之前\r\n    destroyed() {}, //生命周期 - 销毁完成\r\n    activated() {}, //如果页面有keep-alive缓存功能，这个函数会触发\r\n};\r\n</script>\r\n<style lang='less' scoped>\r\n@import url(@/assets/css/Clear_style.css);\r\n@import url(@/assets/css/hero_showcase.css);\r\n</style>"],"mappings":"AA+DA,SAASA,QAAQ,EAAEC,GAAG,EAAEC,MAAK,QAAS,KAAK;AAC3C,SAASC,QAAQ,EAAEC,UAAS,QAAS,MAAM;AAE3C,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAKA,CAAA,EAAG;IACJ,MAAMC,MAAK,GAAI,EAAE;IACjB,MAAMC,IAAG,GAAIT,QAAQ,CAAC;MAClBU,MAAM,EAAE,KAAK;MACbC,QAAQ,EAAE;IACd,CAAC,CAAC;IACF,OAAO;MACH,GAAGT,MAAM,CAACO,IAAI,CAAC;MACfD;IACJ,CAAC;EACL,CAAC;EACD;EACAI,QAAQ,EAAE;IACN;IACA,GAAGT,QAAQ,CAAC,CAAC,QAAQ,CAAC,CAAC;IACvB,GAAGA,QAAQ,CAAC,CAAC,UAAU,CAAC,CAAC;IACzB;IACA,GAAGC,UAAU,CAAC,CAAC,oBAAoB,CAAC;EACxC,CAAC;EACD;EACAS,KAAK,EAAE;IACHH,MAAMA,CAACI,CAAC,EAAEC,CAAC,EAAE;MACT,IAAID,CAAA,KAAM,KAAK,EAAE;QACbE,OAAO,CAACC,GAAG,CAAC,GAAG,CAAC;QAChB,IAAI,CAACT,MAAK,GAAI,IAAI,CAACA,MAAM,CAACU,IAAI,CAC1B,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACE,QAAO,GAAID,CAAC,CAACC,QAC7B,CAAC;MACL,OAAO,IAAIP,CAAA,KAAM,IAAI,EAAE;QACrBE,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC;QACf,IAAI,CAACT,MAAK,GAAI,IAAI,CAACA,MAAM,CAACU,IAAI,CAC1B,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACG,WAAU,GAAIF,CAAC,CAACE,WAChC,CAAC;MACL,OAAO,IAAIR,CAAA,KAAM,IAAI,EAAE;QACrBE,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC;QACf,IAAI,CAACT,MAAK,GAAI,IAAI,CAACA,MAAM,CAACU,IAAI,CAC1B,