{"ast":null,"code":"import axios from \"axios\";\nimport { reactive, toRefs } from \"vue\";\nexport default {\n  name: \"hero_showcase\",\n  components: {},\n  setup() {\n    const data = reactive({\n      heroes: [],\n      region: [],\n      races: [],\n      roles: []\n    });\n    return {\n      ...toRefs(data)\n    };\n  },\n  //监听属性 类似于data概念\n  computed: {},\n  //监控data中的数据变化\n  watch: {},\n  //方法集合\n  methods: {\n    fetchHeroes() {\n      axios.get(\"http://localhost:8089/api/heroes\").then(response => {\n        this.heroes = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    fetchRegion() {\n      axios.get(\"http://localhost:8089/api/region\").then(response => {\n        this.region = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    fetchRaces() {\n      axios.get(\"http://localhost:8089/api/races\").then(response => {\n        this.races = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    fetchRoles() {\n      axios.get(\"http://localhost:8089/api/roles\").then(response => {\n        this.roles = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  },\n  //生命周期 - 创建完成（可以访问当前this实例）\n  created() {},\n  //生命周期 - 挂载完成（可以访问DOM元素）\n  mounted() {\n    this.fetchHeroes();\n    this.fetchRegion();\n    this.fetchRaces();\n    this.fetchRoles();\n  },\n  beforeCreate() {},\n  //生命周期 - 创建之前\n  beforeMount() {},\n  //生命周期 - 挂载之前\n  beforeUpdate() {},\n  //生命周期 - 更新之前\n  updated() {},\n  //生命周期 - 更新之后\n  beforeDestroy() {},\n  //生命周期 - 销毁之前\n  destroyed() {},\n  //生命周期 - 销毁完成\n  activated() {} //如果页面有keep-alive缓存功能，这个函数会触发\n};","map":{"version":3,"names":["axios","reactive","toRefs","name","components","setup","data","heroes","region","races","roles","computed","watch","methods","fetchHeroes","get","then","response","catch","error","console","fetchRegion","fetchRaces","fetchRoles","created","mounted","beforeCreate","beforeMount","beforeUpdate","updated","beforeDestroy","destroyed","activated"],"sources":["D:\\MyCode\\league_of_legends_universe\\src\\views\\hero_showcase.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <div>\r\n    <label for=\"sort-by\">选择排序方式：</label>\r\n    <select v-model=\"sortBy\" id=\"sort-by\">\r\n      <option value=\"name\">按名称</option>\r\n      <option value=\"price\">按价格</option>\r\n      <option value=\"date\">按日期</option>\r\n    </select>\r\n  </div>\r\n        <form action=\"\">\r\n            <input\r\n                type=\"text\"\r\n                name=\"keyword\"\r\n                placeholder=\"输入关键词\"\r\n            >\r\n            <input\r\n                type=\"submit\"\r\n                value=\"搜索\"\r\n            >\r\n        </form>\r\n    </div>\r\n    <div id=\"bk\"></div>\r\n    <div id=\"box_head\">\r\n        <img src=\"../assets/img/public_img/hero_crest_icon.png\">\r\n        <div>\r\n            <h1>英雄</h1>\r\n        </div>\r\n    </div>\r\n    <div id=\"hero_list\">\r\n        <router-link\r\n            v-for=\"(hero,index) in heroes\"\r\n            :key=\"index\"\r\n            :to=\"`/HeroInfo/${hero.formerly}`\"\r\n        >\r\n            <div\r\n                class=\"hero_card\"\r\n                :style=\"{\r\n                  'background-image': 'url(' + require(`../assets/img/hero_posters/${hero.posters}`) + ')',\r\n                  'background-position': `${hero.x*1.0/hero.width*100}% ${hero.y*1.0/hero.height*100}%`,\r\n                  'background-size': 'cover',\r\n                  }\"\r\n            >\r\n                <div>\r\n                    <div>\r\n                        <h2>{{ hero.translation }}</h2>\r\n                        <h1>{{ region.find(item => item.formerly === hero.relevantRegion).translation }}</h1>\r\n                    </div>\r\n                    <div>\r\n                        <p>探索<span>⇢</span></p>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </router-link>\r\n    </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive, toRefs } from \"vue\";\r\nexport default {\r\n    name: \"hero_showcase\",\r\n    components: {},\r\n    setup() {\r\n        const data = reactive({\r\n            heroes: [],\r\n            region: [],\r\n            races: [],\r\n            roles: [],\r\n        });\r\n        return {\r\n            ...toRefs(data),\r\n        };\r\n    },\r\n    //监听属性 类似于data概念\r\n    computed: {},\r\n    //监控data中的数据变化\r\n    watch: {},\r\n    //方法集合\r\n    methods: {\r\n        fetchHeroes() {\r\n            axios\r\n                .get(\"http://localhost:8089/api/heroes\")\r\n                .then((response) => {\r\n                    this.heroes = response.data;\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                });\r\n        },\r\n        fetchRegion() {\r\n            axios\r\n                .get(\"http://localhost:8089/api/region\")\r\n                .then((response) => {\r\n                    this.region = response.data;\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                });\r\n        },\r\n        fetchRaces() {\r\n            axios\r\n                .get(\"http://localhost:8089/api/races\")\r\n                .then((response) => {\r\n                    this.races = response.data;\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                });\r\n        },\r\n        fetchRoles() {\r\n            axios\r\n                .get(\"http://localhost:8089/api/roles\")\r\n                .then((response) => {\r\n                    this.roles = response.data;\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                });\r\n        },\r\n    },\r\n    //生命周期 - 创建完成（可以访问当前this实例）\r\n    created() {},\r\n    //生命周期 - 挂载完成（可以访问DOM元素）\r\n    mounted() {\r\n        this.fetchHeroes();\r\n        this.fetchRegion();\r\n        this.fetchRaces();\r\n        this.fetchRoles();\r\n    },\r\n    beforeCreate() {}, //生命周期 - 创建之前\r\n    beforeMount() {}, //生命周期 - 挂载之前\r\n    beforeUpdate() {}, //生命周期 - 更新之前\r\n    updated() {}, //生命周期 - 更新之后\r\n    beforeDestroy() {}, //生命周期 - 销毁之前\r\n    destroyed() {}, //生命周期 - 销毁完成\r\n    activated() {}, //如果页面有keep-alive缓存功能，这个函数会触发\r\n};\r\n</script>\r\n<style lang='less' scoped>\r\n@import url(../assets/css/Clear_style.css);\r\n@import url(../assets/css/hero_showcase.css);\r\n</style>"],"mappings":"AAyDA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAQ,EAAEC,MAAK,QAAS,KAAK;AACtC,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAKA,CAAA,EAAG;IACJ,MAAMC,IAAG,GAAIL,QAAQ,CAAC;MAClBM,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE;IACX,CAAC,CAAC;IACF,OAAO;MACH,GAAGR,MAAM,CAACI,IAAI;IAClB,CAAC;EACL,CAAC;EACD;EACAK,QAAQ,EAAE,CAAC,CAAC;EACZ;EACAC,KAAK,EAAE,CAAC,CAAC;EACT;EACAC,OAAO,EAAE;IACLC,WAAWA,CAAA,EAAG;MACVd,KAAI,CACCe,GAAG,CAAC,kCAAkC,EACtCC,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAACV,MAAK,GAAIU,QAAQ,CAACX,IAAI;MAC/B,CAAC,EACAY,KAAK,CAAEC,KAAK,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB,CAAC,CAAC;IACV,CAAC;IACDE,WAAWA,CAAA,EAAG;MACVrB,KAAI,CACCe,GAAG,CAAC,kCAAkC,EACtCC,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAACT,MAAK,GAAIS,QAAQ,CAACX,IAAI;MAC/B,CAAC,EACAY,KAAK,CAAEC,KAAK,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB,CAAC,CAAC;IACV,CAAC;IACDG,UAAUA,CAAA,EAAG;MACTtB,KAAI,CACCe,GAAG,CAAC,iCAAiC,EACrCC,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAACR,KAAI,GAAIQ,QAAQ,CAACX,IAAI;MAC9B,CAAC,EACAY,KAAK,CAAEC,KAAK,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB,CAAC,CAAC;IACV,CAAC;IACDI,UAAUA,CAAA,EAAG;MACTvB,KAAI,CACCe,GAAG,CAAC,iCAAiC,EACrCC,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAACP,KAAI,GAAIO,QAAQ,CAACX,IAAI;MAC9B,CAAC,EACAY,KAAK,CAAEC,KAAK,IAAK;QACdC,OAAO,CA