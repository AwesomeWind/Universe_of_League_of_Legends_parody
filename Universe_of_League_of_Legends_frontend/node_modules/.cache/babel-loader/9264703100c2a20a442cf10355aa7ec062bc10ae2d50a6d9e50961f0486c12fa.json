{"ast":null,"code":"import { reactive, toRefs } from \"vue\";\nimport { mapState } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  name: \"hero_information\",\n  components: {},\n  setup() {\n    //这里存放数据\n    const data = reactive({\n      hero: {},\n      region: {},\n      race: {},\n      role: {},\n      update: false\n    });\n    return {\n      ...toRefs(data)\n    };\n  },\n  //计算属性\n  computed: {\n    ...mapState([\"regional\"]),\n    ...mapState([\"races\"]),\n    ...mapState([\"roles\"])\n  },\n  //监控data中的数据变化\n  watch: {\n    hero(n, o) {\n      this.update = !this.update;\n      document.title = n.translation + \" - \" + this.$route.meta.title;\n      this.region = this.regional.find(item => item.formerly === this.hero.relevantRegion);\n      this.race = this.races.find(item => item.formerly === this.hero.races);\n      this.role = this.roles.find(item => item.title === this.hero.role);\n    }\n  },\n  //方法集合\n  methods: {\n    findHero() {\n      axios.get(`http://localhost:8089/api/findHero?formerly=${this.$route.params.heroId}`).then(response => {\n        this.hero = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    dataInitialize() {}\n  },\n  //生命周期 - 创建完成（可以访问当前this实例）\n  created() {\n    this.findHero();\n    this.dataInitialize();\n  },\n  //生命周期 - 挂载完成（可以访问DOM元素）\n  mounted() {},\n  beforeCreate() {},\n  //生命周期 - 创建之前\n  beforeMount() {},\n  //生命周期 - 挂载之前\n  beforeUpdate() {},\n  //生命周期 - 更新之前\n  updated() {},\n  //生命周期 - 更新之后\n  beforeDestroy() {},\n  //生命周期 - 销毁之前\n  destroyed() {},\n  //生命周期 - 销毁完成\n  activated() {} //如果页面有keep-alive缓存功能，这个函数会触发\n};","map":{"version":3,"names":["reactive","toRefs","mapState","axios","name","components","setup","data","hero","region","race","role","update","computed","watch","n","o","document","title","translation","$route","meta","regional","find","item","formerly","relevantRegion","races","roles","methods","findHero","get","params","heroId","then","response","catch","error","console","dataInitialize","created","mounted","beforeCreate","beforeMount","beforeUpdate","updated","beforeDestroy","destroyed","activated"],"sources":["D:\\MyCode\\vueCode\\league_of_legends_universe\\src\\views\\hero_information.vue"],"sourcesContent":["<template>\r\n    <div v-if=\"update\">\r\n        <div\r\n            id=\"head\"\r\n            :style=\"{\r\n              'background-image': `url(${require(`@/assets/img/hero_posters/${hero.posters}`)})`,\r\n              'background-position': `${hero.x/hero.width*100}% ${hero.y/hero.x*100}%`,\r\n              'background-size':'cover'}\"\r\n        >\r\n            <div class=\"head_bk\"></div>\r\n            <div>\r\n                <h1>\r\n                    {{ hero.translation }}\r\n                </h1>\r\n                <img\r\n                    src=\"@/assets/img/public_img/t1HeaderDivider.png\"\r\n                    style=\"width: 100vw\"\r\n                >\r\n                <h2>\r\n                    {{ hero.title }}\r\n                </h2>\r\n            </div>\r\n        </div>\r\n        <div id=\"mind\">\r\n            <div class=\"left\">\r\n                <div\r\n                    v-show=\"hero.races!='null'\"\r\n                    class=\"left_1\"\r\n                >\r\n                    <h2>种族</h2>\r\n                    <span>\r\n                        {{ race.translation }}\r\n                    </span>\r\n                </div>\r\n                <div\r\n                    :style=\"{'height': race.formerly === 'null' ? '635px' : ''}\"\r\n                    class=\"left_2\"\r\n                >\r\n                    <div>\r\n                        <span class=\"box_title\">相关英雄</span>\r\n                    </div>\r\n                    <div></div>\r\n                </div>\r\n            </div>\r\n            <div class=\"min\">\r\n                <div class=\"min_1\">\r\n                    <div class=\"round_logo\">\r\n          