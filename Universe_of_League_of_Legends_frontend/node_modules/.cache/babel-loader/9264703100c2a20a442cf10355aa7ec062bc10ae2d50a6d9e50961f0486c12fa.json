       axios\r\n                .get(\r\n                    `http://localhost:8089/api/findHero?formerly=${this.$route.params.heroId}`\r\n                )\r\n                .then((response) => {\r\n                    this.hero = response.data;\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                });\r\n        },\r\n        dataInitialize() {},\r\n    },\r\n    //生命周期 - 创建完成（可以访问当前this实例）\r\n    created() {\r\n        this.findHero();\r\n        this.dataInitialize();\r\n    },\r\n    //生命周期 - 挂载完成（可以访问DOM元素）\r\n    mounted() {},\r\n    beforeCreate() {}, //生命周期 - 创建之前\r\n    beforeMount() {}, //生命周期 - 挂载之前\r\n    beforeUpdate() {}, //生命周期 - 更新之前\r\n    updated() {}, //生命周期 - 更新之后\r\n    beforeDestroy() {}, //生命周期 - 销毁之前\r\n    destroyed() {}, //生命周期 - 销毁完成\r\n    activated() {}, //如果页面有keep-alive缓存功能，这个函数会触发\r\n};\r\n</script>\r\n<style lang='less' scoped>\r\n@import url(@/assets/css/Clear_style.css);\r\n@import url(@/assets/css/hero_information.css);\r\n</style>"],"mappings":"AAqJA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAK;AACtC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,kBAAkB;EACxBC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAKA,CAAA,EAAG;IACJ;IACA,MAAMC,IAAG,GAAIP,QAAQ,CAAC;MAClBQ,IAAI,EAAE,CAAC,CAAC;MACRC,MAAM,EAAE,CAAC,CAAC;MACVC,IAAI,EAAE,CAAC,CAAC;MACRC,IAAI,EAAE,CAAC,CAAC;MACRC,MAAM,EAAE;IACZ,CAAC,CAAC;IACF,OAAO;MACH,GAAGX,MAAM,CAACM,IAAI;IAClB,CAAC;EACL,CAAC;EACD;EACAM,QAAQ,EAAE;IACN,GAAGX,QAAQ,CAAC,CAAC,UAAU,CAAC,CAAC;IACzB,GAAGA,QAAQ,CAAC,CAAC,OAAO,CAAC,CAAC;IACtB,GAAGA,QAAQ,CAAC,CAAC,OAAO,CAAC;EACzB,CAAC;EACD;EACAY,KAAK,EAAE;IACHN,IAAIA,CAACO,CAAC,EAAEC,CAAC,EAAE;MACP,IAAI,CAACJ,MAAK,GAAI,CAAC,IAAI,CAACA,MAAM;MAC1BK,QAAQ,CAACC,KAAI,GAAIH,CAAC,CAACI,WAAU,GAAI,KAAI,GAAI,IAAI,CAACC,MAAM,CAACC,IAAI,CAACH,KAAK;MAC/D,IAAI,CAACT,MAAK,GAAI,IAAI,CAACa,QAAQ,CAACC,IAAI,CAC3BC,IAAI,IAAKA,IAAI,CAACC,QAAO,KAAM,IAAI,CAACjB,IAAI,CAACkB,cAC1C,CAAC;MACD,IAAI,CAAChB,IAAG,GAAI,IAAI,CAACiB,K