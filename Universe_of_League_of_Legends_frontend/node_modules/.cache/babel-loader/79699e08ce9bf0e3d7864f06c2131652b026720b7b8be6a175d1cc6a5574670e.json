{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive, toRefs } from \"vue\";\nwindow.onload = function () {\n  let login = document.getElementById(\"login\");\n  let enroll = document.getElementById(\"enroll\");\n  let form_box = document.getElementsByClassName(\"form-box\")[0];\n  enroll.addEventListener(\"click\", () => {\n    form_box.style.transform = \"translateX(100%)\";\n  });\n  login.addEventListener(\"click\", () => {\n    form_box.style.transform = \"translateX(0)\";\n  });\n};\nexport default {\n  name: \"login\",\n  components: {},\n  setup() {\n    const data = reactive({\n      show: \"signing\"\n    });\n    return {\n      ...toRefs(data)\n    };\n  },\n  //监听属性 类似于data概念\n  computed: {},\n  //监控data中的数据变化\n  watch: {},\n  //方法集合\n  methods: {\n    handleLogin() {\n      // 发送登录请求\n      this.$axios.post(\"/user/login\", this.loginForm).then(response => {\n        // 登录成功，进行跳转到首页\n        if (response.status === 200 && response.data === 1) {\n          this.$router.push(\"/home\");\n        } else {\n          // 登录失败，显示错误提示信息\n          this.errorMsg = \"用户名或密码错误！\";\n        }\n      }).catch(error => {\n        // 发送请求失败，显示错误提示信息\n        this.errorMsg = \"登录请求发送失败！\";\n      });\n    },\n    handleRegister() {\n      // 发送注册请求\n      this.$axios.post(\"/user/register\", this.registerForm).then(response => {\n        if (response.status === 200) {\n          if (response.data === 1) {\n            // 注册成功，进行跳转到首页\n            this.$router.push(\"/home\");\n          } else {\n            // 注册失败，显示错误提示信息\n            this.errorMsg = \"该用户名已被注册！\";\n          }\n        }\n      }).catch(error => {\n        // 发送请求失败，显示错误提示信息\n        this.errorMsg = \"注册请求发送失败！\";\n      });\n    }\n  },\n  //生命周期 - 创建完成（可以访问当前this实例）\n  created() {},\n  //生命周期 - 挂载完成（可以访问DOM元素）\n  mounted() {},\n  beforeCreate() {},\n  //生命周期 - 创建之前\n  beforeMount() {},\n  //生命周期 - 挂载之前\n  beforeUpdate() {},\n  //生命周期 - 更新之前\n  updated() {},\n  //生命周期 - 更新之后\n  beforeDestroy() {},\n  //生命周期 - 销毁之前\n  destroyed() {},\n  //生命周期 - 销毁完成\n  activated() {} //如果页面有keep-alive缓存功能，这个函数会触发\n};","map":{"version":3,"names":["reactive","toRefs","window","onload","login","document","getElementById","enroll","form_box","getElementsByClassName","addEventListener","style","transform","name","components","setup","data","show","computed","watch","methods","handleLogin","$axios","post","loginForm","then","response","status","$router","push","errorMsg","catch","error","handleRegister","registerForm","created","mounted","beforeCreate","beforeMount","beforeUpdate","updated","beforeDestroy","destroyed","activated"],"sources":["D:\\MyCode\\league_of_legends_universe\\src\\views\\login.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <div \r\n        \r\n        class=\"form-box\">\r\n            <div\r\n                class=\"enroll-box\"\r\n                v-show=\"show === 'signup'\"\r\n            >\r\n                <img\r\n                    src=\"../assets/img/League_of_Legends.png\"\r\n                    alt=\"\"\r\n                >\r\n                <input\r\n                    type=\"text\"\r\n                    placeholder=\"Nema\"\r\n                >\r\n                <input\r\n                    type=\"text\"\r\n                    placeholder=\"Emil\"\r\n                >\r\n                <input\r\n                    type=\"password\"\r\n                    placeholder=\"Password\"\r\n                >\r\n                <input\r\n                    type=\"password\"\r\n                    placeholder=\"Confirm Password\"\r\n                >\r\n                <input\r\n                    type=\"button\"\r\n                    value=\"signup\"\r\n                >\r\n            </div>\r\n            <div\r\n                class=\"login-box \"\r\n                v-show=\"show === 'signing'\"\r\n            >\r\n                <img\r\n                    src=\"../assets/img/League_of_Legends.png\"\r\n                    alt=\"\"\r\n                >\r\n                <input\r\n                    type=\"text\"\r\n                    placeholder=\"Nema\"\r\n                >\r\n                <input\r\n                    type=\"password\"\r\n                    placeholder=\"Password\"\r\n                >\r\n                <input\r\n                    type=\"button\"\r\n                    value=\"signing\"\r\n                >\r\n            </div>\r\n        </div>\r\n        <div class=\"con-box l\">\r\n            <h2>Welcome to the <span>League of Legends universe</span></h2>\r\n            <p>Already have an account?</p>\r\n            <button\r\n                id=\"login\"\r\n                @click=\"toSigning()\"\r\n            >Go to Login</button>\r\n        </div>\r\n        <div class=\"con-box r\">\r\n            <h2>Welcome to the <span>League of Legends universe</span></h2>\r\n            <p>Don't have an account?</p>\r\n            <button\r\n                id=\"enroll\"\r\n                @click=\"toSignup()\"\r\n            >Go to Register</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport { reactive, toRefs } from \"vue\";\r\nwindow.onload = function () {\r\n    let login = document.getElementById(\"login\");\r\n    let enroll = document.getElementById(\"enroll\");\r\n    let form_box = document.getElementsByClassName(\"form-box\")[0];\r\n    enroll.addEventListener(\"click\", () => {\r\n        form_box.style.transform = \"translateX(100%)\";\r\n    });\r\n    login.addEventListener(\"click\", () => {\r\n        form_box.style.transform = \"translateX(0)\";\r\n    });\r\n};\r\nexport default {\r\n    name: \"login\",\r\n    components: {},\r\n    setup() {\r\n        const data = reactive({\r\n            show: \"signing\",\r\n        });\r\n        return {\r\n            ...toRefs(data),\r\n        };\r\n    },\r\n    //监听属性 类似于data概念\r\n    computed: {},\r\n    //监控data中的数据变化\r\n    watch: {},\r\n    //方法集合\r\n    methods: {\r\n        handleLogin() {\r\n            // 发送登录请求\r\n            this.$axios\r\n                .post(\"/user/login\", this.loginForm)\r\n                .then((response) => {\r\n                    // 登录成功，进行跳转到首页\r\n                    if (response.status === 200 && response.data === 1) {\r\n                        this.$router.push(\"/home\");\r\n                    } else {\r\n                        // 登录失败，显示错误提示信息\r\n                        this.errorMsg = \"用户名或密码错误！\";\r\n                    }\r\n                })\r\n                .catch((error) => {\r\n                    // 发送请求失败，显示错误提示信息\r\n                    this.errorMsg = \"登录请求发送失败！\";\r\n                });\r\n        },\r\n        handleRegister() {\r\n            // 发送注册请求\r\n            this.$axios\r\n                .post(\"/user/register\", this.registerForm)\r\n                .then((response) => {\r\n                    if (response.status === 200) {\r\n                        if (response.data === 1) {\r\n                            // 注册成功，进行跳转到首页\r\n                            this.$router.push(\"/home\");\r\n                        } else {\r\n                            // 注册失败，显示错误提示信息\r\n                            this.errorMsg = \"该用户名已被注册！\";\r\n                        }\r\n                    }\r\n                })\r\n                .catch((error) => {\r\n                    // 发送请求失败，显示错误提示信息\r\n                    this.errorMsg = \"注册请求发送失败！\";\r\n                });\r\n        },\r\n    },\r\n    //生命周期 - 创建完成（可以访问当前this实例）\r\n    created() {},\r\n    //生命周期 - 挂载完成（可以访问DOM元素）\r\n    mounted() {},\r\n    beforeCreate() {}, //生命周期 - 创建之前\r\n    beforeMount() {}, //生命周期 - 挂载之前\r\n    beforeUpdate() {}, //生命周期 - 更新之前\r\n    updated() {}, //生命周期 - 更新之后\r\n    beforeDestroy() {}, //生命周期 - 销毁之前\r\n    destroyed() {}, //生命周期 - 销毁完成\r\n    activated() {}, //如果页面有keep-alive缓存功能，这个函数会触发\r\n};\r\n</script>\r\n<style lang='less' scope