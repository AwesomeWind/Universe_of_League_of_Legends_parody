{"ast":null,"code":"import { reactive, toRefs } from \"vue\";\nimport { mapState } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  name: \"hero_information\",\n  components: {},\n  setup() {\n    //这里存放数据\n    const data = reactive({\n      hero: {},\n      region: {},\n      race: {},\n      role: {},\n      relatedPeople: [],\n      update: false\n    });\n    return {\n      ...toRefs(data)\n    };\n  },\n  //计算属性\n  computed: {\n    ...mapState([\"regional\"]),\n    ...mapState([\"races\"]),\n    ...mapState([\"roles\"]),\n    //滚动条高度\n    calculatedHeight() {\n      const parentHeight = this.$refs.parent;\n      console.log(parentHeight);\n      const siblingHeight = this.$refs.sibling.offsetHeight;\n      return parentHeight * 2 - siblingHeight;\n    }\n  },\n  //监控data中的数据变化\n  watch: {\n    async hero(n, o) {\n      document.title = n.translation + \" - \" + this.$route.meta.title;\n      this.region = this.regional.find(item => item.formerly === this.hero.relevantRegion);\n      this.race = this.races.find(item => item.formerly === this.hero.races);\n      this.role = this.roles.find(item => item.title === this.hero.role);\n      if (this.hero.relatedPeople != \"\") {\n        this.hero.relatedPeople = await Promise.all(this.hero.relatedPeople.split(\",\").map(async function (formerly) {\n          try {\n            const response = await axios.get(`http://localhost:8089/api/findHero?formerly=${formerly}`);\n            return response.data;\n          } catch (error) {\n            console.error(error);\n          }\n        }));\n      }\n      this.update = !this.update;\n    }\n  },\n  //方法集合\n  methods: {\n    findHero() {\n      axios.get(`http://localhost:8089/api/findHero?formerly=${this.$route.params.heroId}`).then(response => {\n        this.hero = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    createScrollBar() {\n      const related_people = document.querySelector(\".related_people\");\n      const related_people_height = related_people.offsetHeight;\n      console.log(\"related_people height:\", related_people_height);\n    },\n    addmusemove() {\n      const draggableElement = document.querySelector(\".draggable\");\n      let startY; // 初始 Y 坐标\n      let originalTop; // 元素初始的 top 值\n      let parentHeight; // 父元素的高度\n      let draggableHeight; // 可拖动元素的高度\n      // 鼠标按下时触发\n      draggableElement.addEventListener(\"mousedown\", event => {\n        // 获取元素和父元素的相关信息\n        startY = event.clientY;\n        originalTop = draggableElement.offsetTop;\n        parentHeight = draggableElement.parentNode.offsetHeight;\n        draggableHeight = draggableElement.offsetHeight;\n        // 鼠标移动时触发\n        document.addEventListener(\"mousemove\", handleMouseMove);\n        // 鼠标松开时触发\n        document.addEventListener(\"mouseup\", handleMouseUp);\n      });\n      // 处理鼠标移动事件\n      function handleMouseMove(event) {\n        const deltaY = event.clientY - startY;\n        let newTop = originalTop + deltaY;\n        // 限制元素的 top 值在父元素的高度范围内\n        newTop = Math.max(0, Math.min(newTop, parentHeight - draggableHeight));\n        // 应用新的 top 值\n        draggableElement.style.top = newTop + \"px\";\n      }\n      // 处理鼠标松开事件\n      function handleMouseUp() {\n        document.removeEventListener(\"mousemove\", handleMouseMove);\n        document.removeEventListener(\"mouseup\", handleMouseUp);\n      }\n    }\n  },\n  beforeCreate() {},\n  //生命周期 - 创建之前\n  created() {\n    this.findHero();\n  },\n  //生命周期 - 创建完成（可以访问当前this实例）\n  beforeMount() {},\n  //生命周期 - 挂载之前\n  mounted() {},\n  //生命周期 - 挂载完成（可以访问DOM元素）\n  beforeUpdate() {},\n  //生命周期 - 更新之前\n  updated() {},\n  //生命周期 - 更新之后\n  beforeDestroy() {},\n  //生命周期 - 销毁之前\n  destroyed() {},\n  //生命周期 - 销毁完成\n  activated() {} //如果页面有keep-alive缓存功能，这个函数会触发\n};","map":{"version":3,"names":["reactive","toRefs","mapState","axios","name","components","setup","data","hero","region","race","role","relatedPeople","update","computed","calculatedHeight","parentHeight","$refs","parent","console","log","siblingHeight","sibling","offsetHeight","watch","n","o","document","title","translation","$route","meta","regional","find","item","formerly","relevantRegion","races","roles","Promise","all","split","map","response","get","error","methods","findHero","params","heroId","then","catch","createScrollBar","related_people","querySelector","related_people_height","addmusemove","draggableElement","startY","originalTop","draggableHeight","addEventListener","event","clientY","offsetTop","parentNode","handleMouseMove","handleMouseUp","deltaY","newTop","Math","max","min","style","top","removeEventListener","beforeCreate","created","beforeMount","mounted","beforeUpdate","updated","beforeDestroy","destroyed","activated"],"sources":["D:\\MyCode\\vueCode\\league_of_legends_universe\\src\\views\\hero_information.vue"],"sourcesContent":["<template>\r\n    <div v-if=\"update\">\r\n        <div\r\n            id=\"head\"\r\n            :style=\"{\r\n              'background-image': `url(${require(`@/assets/img/hero_posters/${hero.posters}`)})`,\r\n              'background-position': `${hero.x/hero.width*100}% ${hero.y/hero.x*100}%`,\r\n              'background-size':'cover'}\"\r\n        >\r\n            <div class=\"head_bk\"></div>\r\n            <div>\r\n                <h1>\r\n                    {{ hero.translation }}\r\n                </h1>\r\n                <img\r\n                    src=\"@/assets/img/public_img/t1HeaderDivider.png\"\r\n                    style=\"width: 100vw\"\r\n                >\r\n                <h2>\r\n                    {{ hero.title }}\r\n                </h2>\r\n            </div>\r\n        </div>\r\n        <div id=\"mind\">\r\n            <div class=\"left\">\r\n                <div\r\n                    v-show=\"hero.races!='null'\"\r\n                    class=\"left_1\"\r\n                >\r\n                    <h2>种族</h2>\r\n                    <span>\r\n                        {{ race.translation }}\r\n                    </span>\r\n                </div>\r\n                <div\r\n                    :style=\"{'height': race.formerly === 'null' ? '635px' : ''}\"\r\n                    class=\"left_2\"\r\n                >\r\n                    <div>\r\n                        <span class=\"box_title\">相关英雄</span>\r\n                    </div>\r\n                    <div\r\n                        class=\"related_people\"\r\n                        ref=\"parent\"\r\n                    >\r\n                        <ul ref=\"sibling\">\r\n                            <li\r\n                                v-for=\"(people,index) in this.hero.relatedPeople\"\r\n                                :key=\"index\"\r\n                            >\r\n                                <router-link :to=\"`/HeroInfo/${people.formerly}`\">\r\n                                    <div>\r\n                                        <div :style=\"{\r\n                          'background-image': `url(${require(`../assets/img/hero_posters/${people.posters}`)})`,\r\n                          'background-position': `${people.x/people.width*100}% ${people.y/people.height*100}%`,\r\n                          'background-size':'250%'}\"></div>\r\n                                    </div>\r\n                                    <span>{{ people.translation }}</span>\r\n                                </router-link>\r\n                            </li>\r\n                        </ul>\r\n                        <div\r\n                            :style=\"{'height':200+'px'}\"\r\n                            class=\"scroll_bar\"\r\n                        >\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"min\">\r\n                <div class=\"min_1\">\r\n                    <div class=\"round_logo\">\r\n                        <div class=\"poster_img\">\r\n                            <div :style=\"{\r\n                          'background-image': `url(${require(`@/assets/img/hero_posters/${hero.posters}`)})`,\r\n                          'background-position': `${hero.x/hero.width*100}% ${hero.y/hero.height*100}%`,\r\n                          'background-size':'350%'}\"></div>\r\n                        </div>\r\n                        <img\r\n                            class=\"poster_border\"\r\n                            src=\"../assets/img/public_img/HeroFrame.jpg\"\r\n                        >\r\n                    </div>\r\n                    <h1>{{ hero.quote }}</h1>\r\n                    <h2>~ {{ hero.translation }}</h2>\r\n                </div>\r\n                <div class=\"min_2\">\r\n                    <div class=\"min_2_text\">\r\n                        <p v-html=\"hero.synopsis\">\r\n                        </p>\r\n                        <button>阅读传记<span>⇢</span></button>\r\n                    </div>\r\n                    <div\r\n                        class=\"min_2_bk\"\r\n                        :style=\"{\r\n                          'background-image': `url(${require(`@/assets/img/hero_posters/${hero.posters}`)})`,\r\n                          'background-position': `${hero.x/hero.width*100}% ${hero.y/hero.height*100}%`,\r\n                          'background-size':'cover'}\"\r\n                    >\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"right\">\r\n                <div class=\"right_1\">\r\n                    <div class=\"right_1_logo\">\r\n                        <img\r\n                            class=\"role_icon\"\r\n                            :src=\"require(`@/assets/img/public_img/${role.icon}`)\"\r\n                        >\r\n                    </div>\r\n                    <div class=\"right_1_role\">\r\n                        <p class=\"title_p\">角色定位</p>\r\n                        <span class=\"tile_span\">\r\n                            {{ role.name }}\r\n                        </span>\r\n                    </div>\r\n                </div>\r\n                <div class=\"right_2\">\r\n                    <div class=\"right_2_bk\"></div>\r\n                    <div\r\n                        class=\"right_2_text\"\r\n                        :class=\"{'view_regions':region.formerly!='unaffiliated'}\"\r\n                    >\r\n                        <img\r\n                            class=\"faction_logo\"\r\n                            :src=\"require(`@/assets/img/emblem/${region.emblem}`)\"\r\n                        >\r\n                        <div>\r\n                            <div class=\"region_title\">\r\n                                <p class=\"title_p\">相关地区</p>\r\n                                <span class=\"tile_span\">\r\n                                    {{ region.translation }}\r\n                                </span>\r\n                            </div>\r\n                            <div v-if=\"region.icon!=''\">\r\n                                <img\r\n                                    class=\"faction_icon\"\r\n                                    :src=\"require(`@/assets/img/crest_icon/${region.icon}`)\"\r\n                                >\r\n                            </div>\r\n                        </div>\r\n                        <router-link\r\n                            class=\"relevant_region\"\r\n                            :to=\"`/RegionInfo/${region.formerly}`\"\r\n                        >\r\n                            <div>\r\n                                <p>查看地区<span>⇢</span></p>\r\n                            </div>\r\n                        </router-link>\r\n                    </div>\r\n                </div>\r\n                <div class=\"right_3\">\r\n                    <div\r\n                        class=\"right_3_bk\"\r\n                        :style=\"{\r\n                          'background-image': `url(${require(`@/assets/img/hero_posters/${hero.posters}`)})`,\r\n                          'background-position': `${hero.x/hero.width*100}% ${hero.y/hero.height*100}%`,\r\n                        }\"\r\n                    ></div>\r\n                    <img\r\n                        src=\"@/assets/img/public_img/LOL.png\"\r\n                        alt=\"\"\r\n                    >\r\n                    <ul>\r\n                        <li>\r\n                            <p>游戏信息</p>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"https://lol.qq.com/main.shtml\">\r\n                                <p>查看游戏信息<span>⇢</span></p>\r\n                            </a>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport { reactive, toRefs } from \"vue\";\r\nimport { mapState } from \"vuex\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n    name: \"hero_information\",\r\n    components: {},\r\n    setup() {\r\n        //这里存放数据\r\n        const data = reactive({\r\n            hero: {},\r\n            region: {},\r\n            race: {},\r\n            role: {},\r\n            relatedPeople: [],\r\n            update: false,\r\n        });\r\n        return {\r\n            ...toRefs(data),\r\n        };\r\n    },\r\n    //计算属性\r\n    computed: {\r\n        ...mapState([\"regional\"]),\r\n        ...mapState([\"races\"]),\r\n        ...mapState([\"roles\"]),\r\n        //滚动条高度\r\n        calculatedHeight() {\r\n            const parentHeight = this.$refs.parent;\r\n            console.log(parentHeight);\r\n            const siblingHeight = this.$refs.sibling.offsetHeight;\r\n            return parentHeight * 2 - siblingHeight;\r\n        },\r\n    },\r\n    //监控data中的数据变化\r\n    watch: {\r\n        async hero(n, o) {\r\n            document.title = n.translation + \" - \" + this.$route.meta.title;\r\n            this.region = this.regional.find(\r\n                (item) => item.formerly === this.hero.relevantRegion\r\n            );\r\n            this.race = this.races.find(\r\n                (item) => item.formerly === this.hero.races\r\n            );\r\n            this.role = this.roles.find(\r\n                (item) => item.title === this.hero.role\r\n            );\r\n            if (this.hero.relatedPeople != \"\") {\r\n                this.hero.relatedPeople = await Promise.all(\r\n                    this.hero.relatedPeople\r\n                        .split(\",\")\r\n                        .map(async function (formerly) {\r\n                            try {\r\n                                const response = await axios.get(\r\n                                    `http://localhost:8089/api/findHero?formerly=${formerly}`\r\n                                );\r\n                                return response.data;\r\n                            } catch (error) {\r\n                                console.error(error);\r\n                            }\r\n                        })\r\n                );\r\n            }\r\n            this.update = !this.update;\r\n        },\r\n    },\r\n    //方法集合\r\n    methods: {\r\n        findHero() {\r\n            axios\r\n                .get(\r\n                    `http://localhost:8089/api/findHero?formerly=${this.$route.params.heroId}`\r\n                )\r\n                .then((response) => {\r\n                    this.hero = response.data;\r\n                })\r\n                .catch((error) => {\r\n                    console.error(error);\r\n                });\r\n        },\r\n        createScrollBar() {\r\n            const related_people = document.querySelector(\".related_people\");\r\n            const related_people_height = related_people.offsetHeight;\r\n            console.log(\"related_people height:\", related_people_height);\r\n        },\r\n        addmusemove() {\r\n            const draggableElement = document.querySelector(\".draggable\");\r\n            let startY; // 初始 Y 坐标\r\n            let originalTop; // 元素初始的 top 值\r\n            let parentHeight; // 父元素的高度\r\n            let draggableHeight; // 可拖动元素的高度\r\n            // 鼠标按下时触发\r\n            draggableElement.addEventListener(\"mousedown\", (event) => {\r\n                // 获取元素和父元素的相关信息\r\n                startY = event.clientY;\r\n                originalTop = draggableElement.offsetTop;\r\n                parentHeight = draggableElement.parentNode.offsetHeight;\r\n                draggableHeight = draggableElement.offsetHeight;\r\n                // 鼠标移动时触发\r\n                document.addEventListener(\"mousemove\", handleMouseMove);\r\n                // 鼠标松开时触发\r\n                document.addEventListener(\"mouseup\", handleMouseUp);\r\n            });\r\n            // 处理鼠标移动事件\r\n            function handleMouseMove(event) {\r\n                const deltaY = event.clientY - startY;\r\n                let newTop = originalTop + deltaY;\r\n                // 限制元素的 top 值在父元素的高度范围内\r\n                newTop = Math.max(\r\n                    0,\r\n                    Math.min(newTop, parentHeight - draggableHeight)\r\n                );\r\n                // 应用新的 top 值\r\n                draggableElement.style.top = newTop + \"px\";\r\n            }\r\n            // 处理鼠标松开事件\r\n            function handleMouseUp() {\r\n                document.removeEventListener(\"mousemove\", handleMouseMove);\r\n                document.removeEventListener(\"mouseup\", handleMouseUp);\r\n            }\r\n        },\r\n    },\r\n    beforeCreate() {}, //生命周期 - 创建之前\r\n    created() {\r\n        this.findHero();\r\n    }, //生命周期 - 创建完成（可以访问当前this实例）\r\n    beforeMount() {}, //生命周期 - 挂载之前\r\n    mounted() {}, //生命周期 - 挂载完成（可以访问DOM元素）\r\n    beforeUpdate() {}, //生命周期 - 更新之前\r\n    updated() {}, //生命周期 - 更新之后\r\n    beforeDestroy() {}, //生命周期 